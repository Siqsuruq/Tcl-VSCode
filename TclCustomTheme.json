{
    "name": "Tcl/Tk Theme",
    "type": "dark",
    "tokenColors": [
        {
            "scope": "comment.line.tcl",
            "settings":{
                "foreground":"#2D6A4F"
            }
        },
        {
            "scope": "commands.tcl",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#457BDD"
            }
        },
        {
            "scope":"command.option.tcl",
            "settings":{
                "fontStyle":"italic",
                "foreground":"#F1FAEE"
            }
        },
        {
            "scope": "namespace.command.tcl",
            "settings":{
                "foreground":"#A8DADC"
            }
        },
        {
            "scope": "package.subcommand.tcl",
            "settings":{
                "foreground":"#457B9D"
            }
        },
        {
            "scope": "dict.subcommand.tcl",
            "settings":{
                "foreground":"#457B9D"
            }
        },
        {
            "scope":"variable.name.tcl",
            "settings":{
                "foreground":"#CC535D"
            }
        },
        {
            "scope": "variable.other.tcl",
            "settings":{
                "foreground":"#CC535D"
            }
        },
        {
            "scope": "string.quoted.double.tcl",
            "settings":{
                "foreground":"#A8DADC"
            }
        },
        {
            "scope": "ns.commands.tcl",
            "settings":{
                "foreground":"#00FFBB"
            }
        }
    ]
}

