{
  "name": "modern-tcl-tk",
  "displayName": "Modern Tcl/Tk",
  "description": "Modern Tcl/Tk Language support for VS Code",
  "version": "0.0.1",
  "publisher": "Maksym Zinchenko",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Siqsuruq/Tcl-VSCode"
  },
  "contributes": {
    "languages": [{
      "id": "tcl",
      "aliases": ["Tcl", "tcl"],
      "extensions": [".tcl",".tk",".tclapp",".tkapp",".tm",".tclclass",".tclobj"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "tcl",
      "scopeName": "source.tcl",
      "path": "./syntaxes/tcl.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "tcl",
        "path": "./snippets/tcl.json"
      }
    ]
  }
}
